body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
form,
fieldset,
input,
p,
blockquote,
th,
td
  margin: 0
  padding: 0
/* 去除轮廓线 */
button,
input,
select,
textarea
  outline: none

button,
a,
label,
input[type='check']
  cursor: pointer

html,
body
  height: 100%

body
  font:12px/1.5 Microsoft YaHei,Heiti SC,tahoma,arial,Hiragino Sans GB,\\5B8B\4F53,sans-serif
  color:#333
  background:#f0f3ef
ol,
ul
  list-style: none
em,
i
  font-style: normal
textarea
  resize: none
a,
a:hover
  text-decoration: none
  color: #333
img
  vertical-align: middle
  border: none;
table
  border-collapse: collapse;
  border-spacing: 0
input,
button
  border: none
/* 清除浮动 */
.clearfix:after
  content: ""
  display: block
  height: 0
  clear: both
  visibility: hidden
.clearfix
  *zoom: 1