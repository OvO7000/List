div.v-dialog
  position: fixed
  top: 0
  right: 0
  bottom: 0
  left: 0
  z-index: 1050
  overflow: hidden
  -webkit-overflow-scrolling: touch
  outline: 0
  display: flex
  justify-content: center
  align-items: center
  &.v-dialog-open 
    overflow: hidden
  div.v-dialog-dialog
    //position: relative
    //margin: 50% auto 0
    //width: 100%
    div.v-dialog-content
      display: flex
      flex-wrap: nowrap
      background-color: #797979
      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5)
      overflow: hidden
      div.v-dialog-center
        flex: 0 0 auto
        div.v-dialog-header
          width: 100%
          h3
            font-weight: normal
            line-height: 30px
            font-size: 16px
      button
        flex: 0 0 100px
        width: 100px
        transition: all .5s cubic-bezier(.175, .885, .32, 1)
        opacity: 0.3
        color: #ccc
        &:hover
          opacity: 1
          background-color: #717171
        span
          font-size: 14px
